@{
}

@model List<NewMovie>

<table class="table table-bordered">
    <thead class="fw-bold">
        <tr>
            <td>Category</td>
            <td>Title</td>
            <td>Year</td>
            <td>Director</td>
            <td>Rating</td>
            <td>Lent To</td>
            <td>Edited</td>
            <td>Notes</td>
        </tr>
        
    </thead>
    @foreach (var temp in Model)
    {
       <tbody>
           <tr>
                <td>@temp.Category</td>
                <td>@temp.Title</td>
                <td>@temp.Year</td>
                <td>@temp.Director</td>
                <td>@temp.Rating</td>
                <td>@temp.LentTo</td>
                <td>@temp.Edited</td>
                <td>@temp.Notes</td>
           </tr>
        
    </tbody> 
    }
    
</table>